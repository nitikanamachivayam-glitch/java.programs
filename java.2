import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        String[] students = {"Nitika", "Arun", "Karthik", "Priya"};

        // Sort alphabetically
        Arrays.sort(students);

        // Display sorted names
        for (String name : students) {
            System.out.println(name);
        }
    }
}
